import java.util.Scanner;

public class L4_10810 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int M = sc.nextInt();
		int[] num = new int[N+1];
		
		for (int m  = 0; m<M; m++) {
			int i = sc.nextInt();
			int j = sc.nextInt();
			int k = sc.nextInt();
			
			for (int n = i; n<=j;n++) {
				num[n] = k;
			}
		}
		
		for(int n = 1; n<=N ; n++) {
			System.out.print(num[n]+" ");
		}
		
		
		sc.close();
	}

}
