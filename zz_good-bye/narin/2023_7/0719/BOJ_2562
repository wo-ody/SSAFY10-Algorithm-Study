import java.util.Scanner;

public class L4_2562 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] num= new int[9];
		int max = 0;
		int maxidx = 0;
		for(int i = 0; i<9;i++) {
			num[i] = sc.nextInt();
			if(num[i]>max) {
				max = num[i];
				maxidx = i+1;
			}
		}
		System.out.println(max);
		System.out.println(maxidx);
		
		sc.close();
	}
}
